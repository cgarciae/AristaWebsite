body
  margin 0
  padding 0

nav
  position fixed
  width 100%
  display flex
  justify-content center
  ul
    margin 0
    padding 0
    width 60%
    list-style none
    display flex
    flex-direction row
    justify-content space-between

#slideshow
  overflow hidden
  ul
    width 2500px
    list-style none
    margin 0
    padding 0

    li
      float left

#canvas
  background-color #5973C0

#Page2
  background-color #B1B1B1

#Page3
  background-color #CE608C

#Page4
  background-color #59C093

#Page5
  background-color #60CEC5

